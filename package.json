{
  "name": "meetup-next",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "db:generate": "pnpx prisma generate",
    "db:migrate": "pnpx prisma migrate dev",
    "db:reset": "pnpx prisma migrate reset",
    "db:pull": "pnpx prisma db pull",
    "db:push": "pnpx prisma db push",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage",
    "prepare": "husky install"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^3.0.1",
    "@hookform/resolvers": "^3.2.0",
    "@prisma/client": "^5.1.1",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-slot": "^1.0.2",
    "autoprefixer": "10.4.14",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.263.1",
    "next": "13.4.13",
    "prisma-extension-pagination": "^0.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.45.4",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "3.3.3",
    "tailwindcss-animate": "^1.0.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@faker-js/faker": "^8.0.2",
    "@ianvs/prettier-plugin-sort-imports": "^4.1.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.3",
    "@types/node": "20.4.8",
    "@types/react": "18.2.18",
    "@types/react-dom": "18.2.7",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.12",
    "husky": "^8.0.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "postcss": "8.4.27",
    "prisma": "^5.1.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6",
    "zod": "^3.21.4"
  }
}
